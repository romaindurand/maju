{
  "name": "maju",
  "version": "3.0.0-beta.1",
  "description": "maju (for MAjority JUdgment) is a Javascript library implementing the Majority Judgment voting system.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "demo": "vite --config vite.config.ts",
    "demo:node": "node demo/index.js",
    "test": "vitest run --config vitest.config.ts",
    "prepare": "pnpm run build"
  },
  "keywords": [
    "poll",
    "majority judgment"
  ],
  "author": "@romaindurand",
  "website": "https://maju.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romaindurand/maju.git"
  },
  "license": "ISC",
  "devDependencies": {
    "ava": "1.0.0-beta.5.1",
    "npm-run-all": "^4.1.3",
    "parcel": "^1.8.1",
    "redom": "^3.12.3",
    "tinygradient": "^0.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vitest": "^4.0.14"
  },
  "files": [
    "dist"
  ]
}